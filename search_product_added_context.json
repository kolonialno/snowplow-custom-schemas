{
  "description": "Describing the event where a product is added from a search result list",
  "self": {
    "vendor": "com.oda",
    "name": "search_product_added_context",
    "format": "jsonschema",
    "version": "1-0-0"
  },
  "type": "object",
  "properties": {
    "query": {
      "description": "The search query string",
      "type": "string",
      "maxLength": 256
    },
    "list_position": {
      "description": "At what index in the search result was the product added from",
      "type": "integer",
      "maxLength": 256,
      "minimum": 0
    },
    "product_id": {
      "description": "Which product was added",
      "type": "integer",
      "minimum": 0
    }
  },
  "additionalProperties": false,
  "required": [
    "query", "product_id"
  ]
}
